{
	"name": "makeitpop",
	"version": "0.0.1",
	"scripts": {
		"dev:only": "svelte-kit dev",
		"build:only": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/styles/global.css -o src/styles/global-output.css -w",
		"tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/styles/global.css -o src/styles/global-output.css",
		"dev": "concurrently \"yarn run dev:only\" \"yarn run tailwind:watch\"",
		"build": "yarn run tailwind:build && yarn run build:only"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"autoprefixer": "^10.4.2",
		"concurrently": "^7.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"postcss-cli": "^9.1.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.44.0",
		"tailwindcss": "^3.0.11"
	},
	"type": "module",
	"dependencies": {
		"hammerjs": "^2.0.8",
		"pexels": "^1.3.0",
		"postcss-load-config": "^3.1.1",
		"svelte-inline-svg": "^1.0.8",
		"svelte-preprocess": "^4.10.1"
	}
}
